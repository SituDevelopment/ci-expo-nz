        "@eslint/eslintrc": "^3.2.0",
        "@types/node": "^22.5.4",
        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "eslint": "^9.16.0",
        "eslint-config-next": "^15.1.7",
        "prettier": "^3.4.2",
        "typescript": "5.7.3"
    },
    "engines": {
        "node": "^18.20.2 || >=20.9.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "sharp"
        ]
    }
