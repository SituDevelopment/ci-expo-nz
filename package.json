        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "class-variance-authority": "^0.7.1",
        "cross-env": "^7.0.3",
        "date-fns": "^4.1.0",
        "graphql": "^16.8.1",
        "lucide-react": "^0.475.0",
        "motion": "^12.4.7",
        "next": "^15.1.7",
        "next-sitemap": "^4.2.3",
        "payload": "^3.24.0",
        "postcss": "^8.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
